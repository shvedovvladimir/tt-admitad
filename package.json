{
  "name": "tt-admitad",
  "version": "0.0.1",
  "description": "tt-admitad",
  "author": "vashvedov",
  "repository": "git@github.com:shvedovvladimir/tt-admitad.git",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jest.config.json",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:cov": "jest --coverage",
    "start": "node --max-old-space-size=900 src/index",
    "lint": "tslint --fix",
    "ci-lint": "tslint",
    "build": "tsc --project ./tsconfig.json",
    "debug": "ts-node-dev --inspect=0.0.0.0 --all-deps --ignore-watch node_modules  --respawn --transpileOnly src/index.ts",
    "dev": "ts-node-dev --inspect=0.0.0.0 --prefer-ts --all-deps --ignore-watch node_modules  --respawn --transpileOnly src/index.ts"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/ioredis": "^4.14.8",
    "@types/node-cache": "^4.2.5",
    "@types/redis": "^2.8.16",
    "cls-hooked": "^4.2.2",
    "config": "2.0.1",
    "cookie-parser": "^1.4.3",
    "got": "^9.6.0",
    "ioredis": "^4.16.0",
    "moment": "^2.24.0",
    "node-gyp": "^6.1.0",
    "pg": "^7.12.1",
    "promisify": "0.0.3",
    "redis": "^3.0.2",
    "redis-streams": "^1.1.0",
    "redislock": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "snakecase-keys": "^3.1.2",
    "swagger-ui-dist": "^3.24.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.18",
    "uuid": "^7.0.2",
    "validator": "^12.2.0",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/config": "0.0.34",
    "@types/express": "^4.17.1",
    "@types/got": "^9.6.9",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.29",
    "@types/pg": "^7.11.0",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "nock": "^12.0.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.42",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
